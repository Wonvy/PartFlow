{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@partflow/core": [
        "packages/core/src"
      ],
      "@partflow/utils": [
        "packages/utils/src"
      ],
      "@partflow/ui": [
        "packages/ui/src"
      ],
      "@partflow/server": [
        "packages/server/src"
      ],
      "@partflow/ai": [
        "packages/ai/src"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}

